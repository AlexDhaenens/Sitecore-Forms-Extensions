@using Sitecore.ExperienceForms.Mvc.Html
@model Feature.FormsExtensions.Fields.Hidden.HiddenViewModel
@if (!Sitecore.Context.PageMode.IsNormal)
<input id="@Html.IdFor(m => Model.Value)" name="@Html.NameFor(m => Model.Value)" type="hidden" value="@Model.Value" data-sc-tracking="@Model.IsTrackingEnabled" data-sc-field-name="@Model.Name" />